<Window x:Class="MagyarorszagTerkepe.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MagyarorszagTerkepe"
        mc:Ignorable="d"
        Title="Magyarország térképe" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="3*"/>
            <ColumnDefinition Width="29*"/>
        </Grid.ColumnDefinitions>
        <Canvas x:Name="Rajzlap" Grid.ColumnSpan="2" HorizontalAlignment="Left" Height="430" Margin="0,0,0,0" VerticalAlignment="Center" Width="800" Background="Transparent" MouseUp="Terkep"/>

    </Grid>
</Window>
